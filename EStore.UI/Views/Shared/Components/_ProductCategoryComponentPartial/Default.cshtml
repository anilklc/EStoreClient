@model List<ResultCategory>
<div class="sidebar-widget category">
    <h2 class="title">Category</h2>
    <nav class="navbar bg-light">
        <ul class="navbar-nav">
            @foreach (var item in Model)
            {
                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Index", new { categoryName = @item.CategoryName.Equals("Home") ? "" : @item.CategoryName })"><i class="@item.CategoryIcon"></i>@item.CategoryName</a>
                </li>
            }
        </ul>
    </nav>
</div>