@model List<ResultSocialMedia>
<div class="col-lg-3 col-md-6">
    <div class="footer-widget">
        <h2>Follow Us</h2>
        <div class="contact-info">
            <div class="social">
                @foreach (var item in Model)
                {
                    <a href="@item.SocialMediaUrl"><i class="@item.SocialMediaIcon"></i></a>

                }
            </div>
        </div>
    </div>
</div>