@model List<ResultBrandWithImages>
<div class="brand">
    <div class="container-fluid">
        <div class="brand-slider">
            @foreach (var item in Model)
            {
                <div class="brand-item"><img width="150" height="80" src="@item.ImagePath" href="/Products?brandName=@item.BrandName" alt="@item.BrandName"></div>
            }
        </div>
    </div>
</div>
