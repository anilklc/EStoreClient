@model UpdateOrderCargo
@{
    ViewData["Title"] = "UpdateOrderCargo";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="container-fluid pt-4 px-4">
    <div class="row vh-100 bg-secondary rounded ">
        <div class="col-md-12">
            <div class="col-sm-12 col-xl-12">
                <div class="bg-secondary rounded h-100 p-4">
                    <h6 class="mb-4">Update Cargo Tracking</h6>
                    <form method="post">
                        <input type="hidden" asp-for="Id" />
                        <div class="mb-3">
                            <label for="exampleInputCargoTracking" class="form-label">Cargo Tracking</label>
                            <input type="text" class="form-control" id="exampleInputCargoTracking" asp-for="CargoTracking" aria-describedby="CargoTracking">
                        </div>
                        <div class="text-center">
                            <button type="submit" class="btn btn-success">Update Order Cargo Tracking</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>


